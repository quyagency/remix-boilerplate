{
  "private": true,
  "name": "remix-boilerplate",
  "description": "Fastest FE Site",
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "postinstall": "remix setup cloudflare-workers",
    "tailwind": "tailwindcss -i app/assets/css/style.css -o ./app/style.css -c ./tailwind.config.js --postcss",
    "dev:tailwind": "yarn run tailwind --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev": "concurrently 'yarn:dev:*'",
    "prebuild": "rimraf .cache dist build public/build",
    "build": "cross-env NODE_ENV=production yarn run tailwind --minify && remix build",
    "deploy": "yarn run build && wrangler publish -e production",
    "svgr": "svgr app/assets/svg/ -d app/icons/ --ext tsx --icon"
  },
  "dependencies": {
    "@elgorditosalsero/react-gtm-hook": "2.3.1",
    "@headlessui/react": "1.5.0",
    "@paypal/react-paypal-js": "^7.6.0",
    "@remix-run/cloudflare-workers": "1.2.3",
    "@remix-run/react": "1.2.3",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.23.0",
    "@wordpress/hooks": "3.3.1",
    "ahooks": "3.1.10",
    "axios": "0.26.0",
    "canvas-confetti": "^1.5.1",
    "clsx": "^1.1.1",
    "cookie": "^0.4.2",
    "currency.js": "^2.0.4",
    "date-fns": "^2.28.0",
    "headroom.js": "^0.12.0",
    "is-mobile": "3.1.0",
    "isbot": "3.4.3",
    "js-cookie": "^3.0.1",
    "query-string": "^7.1.1",
    "ramda": "0.28.0",
    "ramda-adjunct": "3.0.0",
    "ramda-extension": "^0.11.0",
    "rc-textarea": "^0.3.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "7.27.1",
    "react-hot-toast": "^2.2.0",
    "react-icons": "^4.3.1",
    "react-lazyload": "^3.2.0",
    "react-slick": "^0.28.1",
    "react-sweet-state": "^2.5.2",
    "remix": "1.2.3",
    "remix-utils": "2.5.0",
    "slick-carousel": "^1.8.1",
    "slugify": "^1.6.5",
    "tiny-emitter": "^2.1.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.4.0",
    "@remix-run/dev": "1.2.3",
    "@svgr/cli": "6.2.1",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "0.3.1",
    "@tailwindcss/typography": "0.5.2",
    "@types/js-cookie": "^3.0.1",
    "@types/ramda": "0.28.0",
    "@types/react": "17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "@vunamhung/slimui": "^0.10.1",
    "babel-eslint": "^10.1.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "8.10.0",
    "eslint-config-react-app": "7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "miniflare": "2.3.0",
    "postcss": "8.4.7",
    "postcss-import": "^14.0.2",
    "postcss-import-ext-glob": "^2.0.1",
    "postcss-inline-svg": "^5.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "rimraf": "^3.0.2",
    "schema-dts": "1.1.0",
    "tailwindcss": "3.0.23",
    "typescript": "4.6.2"
  },
  "engines": {
    "node": ">=16.7"
  },
  "sideEffects": false
}
