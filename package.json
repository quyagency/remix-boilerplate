{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "remix build",
    "build:worker": "esbuild --define:process.env.NODE_ENV='\"production\"' --minify --bundle --sourcemap --outdir=dist ./worker",
    "deploy": "yarn run build && wrangler publish -e development",
    "deploy:preview": "yarn run build && wrangler dev -e development",
    "deploy:production": "yarn run build && wrangler publish -e production",
    "dev:tailwind": "tailwindcss -i app/assets/css/style.css -o ./app/style.css --postcss -w",
    "dev:worker": "esbuild --define:process.env.NODE_ENV='\"development\"' --bundle --sourcemap --outdir=dist ./worker",
    "dev:miniflare": "miniflare --build-command 'yarn run dev:worker' --watch",
    "dev:remix": "remix watch",
    "dev": "NODE_ENV=development concurrently 'yarn:dev:*'",
    "prebuild": "tailwindcss -i app/assets/css/style.css -o ./app/style.css --postcss --minify",
    "postinstall": "remix setup cloudflare-workers",
    "start": "yarn run dev:miniflare",
    "svgr": "svgr app/assets/svg/ -d app/icons/ --ext tsx --icon"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@remix-run/cloudflare-workers": "^1.1.1",
    "@remix-run/react": "^1.1.1",
    "clsx": "^1.1.1",
    "mapbox-gl": "^2.6.1",
    "ramda": "^0.27.1",
    "ramda-extension": "^0.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-sweet-state": "^2.5.2",
    "remix": "^1.1.1",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.3.0",
    "@remix-run/dev": "^1.1.1",
    "@svgr/cli": "^6.2.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/mapbox-gl": "^2.6.0",
    "@types/ramda": "^0.27.62",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vunamhung/slimui": "^0.10.1",
    "babel-eslint": "^10.1.0",
    "concurrently": "^7.0.0",
    "esbuild": "0.14.11",
    "eslint": "7.32.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "miniflare": "2.0.0-next.3",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-import-ext-glob": "^2.0.1",
    "postcss-inline-svg": "^5.0.0",
    "tailwindcss": "^3.0.12",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "dist/worker.js"
}
