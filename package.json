{
  "name": "remix-boilerplate",
  "description": "Fastest FE Site",
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "postinstall": "patch-package",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare --watch",
    "dev": "concurrently 'npm:dev:*'",
    "prebuild": "rimraf .cache dist build public/build",
    "build": "cross-env NODE_ENV=production remix build",
    "deploy": "wrangler publish -e production",
    "deploy:dev": "wrangler publish",
    "svgr": "svgr app/assets/svg/ -d app/icons/ --ext tsx --icon"
  },
  "dependencies": {
    "@mantine/hooks": "^5.10.0",
    "@ngneat/falso": "^6.3.2",
    "@remix-run/cloudflare": "^1.15.0",
    "@remix-run/cloudflare-workers": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@tabler/icons": "^1.119.0",
    "@tsndr/cloudflare-worker-jwt": "^2.1.4",
    "axios": "^1.2.2",
    "clsx": "^1.2.1",
    "cookie": "0.5.0",
    "currency.js": "^2.0.4",
    "headroom.js": "^0.12.0",
    "js-cookie": "^3.0.1",
    "nprogress": "^0.2.0",
    "patch-package": "^6.5.1",
    "query-string": "^8.1.0",
    "radash": "^10.3.2",
    "ramda": "0.28.0",
    "ramda-adjunct": "^3.4.0",
    "rc-textarea": "^0.4.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.41.5",
    "react-hot-toast": "^2.4.0",
    "remix-utils": "^5.1.0",
    "slugify": "^1.6.5",
    "sprintf-js": "^1.1.2",
    "tiny-emitter": "^2.1.0",
    "valtio": "^1.8.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@svgr/cli": "^6.5.1",
    "@tailwindcss/typography": "^0.5.8",
    "@types/js-cookie": "3.0.2",
    "@types/nprogress": "^0.2.0",
    "@types/ramda": "^0.28.20",
