{
  "name": "remix-boilerplate",
  "description": "Fastest FE Site",
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "postinstall": "remix setup cloudflare-workers",
    "tailwind": "tailwindcss -i app/assets/css/style.css -o ./app/style.css -c ./tailwind.config.js --postcss",
    "dev:tailwind": "yarn run tailwind --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev": "concurrently 'yarn:dev:*'",
    "prebuild": "rimraf .cache dist build public/build",
    "build": "cross-env NODE_ENV=production yarn run tailwind --minify && remix build",
    "deploy": "yarn run build && wrangler publish -e production",
    "svgr": "svgr app/assets/svg/ -d app/icons/ --ext tsx --icon"
  },
  "dependencies": {
    "@elgorditosalsero/react-gtm-hook": "2.4.0",
    "@headlessui/react": "1.6.4",
    "@paypal/react-paypal-js": "7.8.1",
    "@remix-run/cloudflare": "1.5.1",
    "@remix-run/cloudflare-workers": "1.5.1",
    "@remix-run/react": "1.5.1",
    "@stripe/react-stripe-js": "1.8.1",
    "@stripe/stripe-js": "1.31.0",
    "@wordpress/hooks": "3.10.0",
    "ahooks": "3.4.1",
    "axios": "0.27.2",
    "canvas-confetti": "^1.5.1",
    "clsx": "^1.1.1",
    "cookie": "0.5.0",
    "currency.js": "^2.0.4",
    "date-fns": "^2.28.0",
    "filepond-plugin-file-validate-size": "2.2.7",
    "filepond-plugin-file-validate-type": "1.2.8",
    "filepond-plugin-image-exif-orientation": "^1.0.11",
    "filepond-plugin-image-preview": "^4.6.11",
    "headroom.js": "^0.12.0",
    "is-mobile": "3.1.1",
    "isbot": "3.5.0",
    "js-cookie": "^3.0.1",
    "query-string": "^7.1.1",
    "ramda": "0.28.0",
    "ramda-adjunct": "3.1.0",
    "ramda-extension": "^0.11.0",
    "rc-textarea": "^0.3.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-filepond": "7.1.2",
    "react-hook-form": "7.32.0",
    "react-hot-toast": "^2.2.0",
    "react-icons": "4.4.0",
    "react-lazyload": "^3.2.0",
    "react-slick": "0.29.0",
    "react-sweet-state": "2.6.0",
    "remix-utils": "3.3.0",
    "slick-carousel": "^1.8.1",
    "slugify": "^1.6.5",
    "sprintf-js": "^1.1.2",
    "tiny-emitter": "^2.1.0",
    "validator": "^13.7.0"
