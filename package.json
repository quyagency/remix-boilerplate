{
  "private": true,
  "name": "remix-app-template",
  "description": "Fastest FE Site",
  "license": "MIT",
  "main": "dist/worker.js",
  "scripts": {
    "postinstall": "remix setup cloudflare-workers",
    "tailwind": "tailwindcss -i app/assets/css/style.css -o ./app/style.css -c ./tailwind.config.js --postcss",
    "dev:tailwind": "yarn run tailwind --watch",
    "dev:remix": "remix watch",
    "dev:miniflare": "miniflare --build-command './build_dev.mjs' --watch --debug",
    "dev": "concurrently 'yarn:dev:*'",
    "prebuild": "rimraf .cache ./dist ./build ./public/build ./app/style.css",
    "build": "yarn run tailwind --minify && remix build",
    "deploy": "yarn run build && wrangler publish",
    "svgr": "svgr app/assets/svg/ -d app/icons/ --ext tsx --icon"
  },
  "dependencies": {
    "@elgorditosalsero/react-gtm-hook": "^2.3.0",
    "@headlessui/react": "^1.4.2",
    "@remix-run/cloudflare-workers": "^1.1.1",
    "@remix-run/react": "^1.1.1",
    "@wordpress/hooks": "^3.2.2",
    "clsx": "^1.1.1",
    "js-cookie": "^3.0.1",
    "mapbox-gl": "^2.6.1",
    "query-string": "^7.1.0",
    "ramda": "0.27.2",
    "ramda-adjunct": "^2.35.0",
    "ramda-extension": "^0.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-lazyload": "^3.2.0",
    "react-sweet-state": "^2.5.2",
    "remix": "^1.1.1",
    "remix-utils": "^2.4.0",
    "remix-validated-form": "^3.1.1",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.3.0",
    "@remix-run/dev": "^1.1.1",
    "@svgr/cli": "^6.2.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "0.3.1",
    "@tailwindcss/typography": "^0.5.0",
    "@types/js-cookie": "^3.0.1",
    "@types/mapbox-gl": "^2.6.0",
    "@types/ramda": "0.27.63",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "@vunamhung/slimui": "^0.10.1",
    "babel-eslint": "^10.1.0",
    "concurrently": "^7.0.0",
    "esbuild": "0.14.11",
    "eslint": "8.6.0",
    "eslint-config-react-app": "7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "miniflare": "2.0.0",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-import-ext-glob": "^2.0.1",
    "postcss-inline-svg": "^5.0.0",
    "rimraf": "^3.0.2",
    "schema-dts": "^1.0.0",
    "tailwindcss": "3.0.15",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16.7"
  },
  "sideEffects": false
}
